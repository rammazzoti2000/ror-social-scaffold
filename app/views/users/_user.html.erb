<% @users.each do |user| %>
  <% if current_user != user %>
    <li>
      <% if current_user.friend_requests.any?(user) %>
        <%= confirm_button(user) %>
      <% else %>
        <%= friendship_button(user) %>
      <% end %>

      Name: <%= user.name %>
      <span class="profile-link">
        <%= link_to 'See Profile',  user_path(user), class: 'profile-link' %>
      </span>
    </li>
  <% end %>
<% end %>
